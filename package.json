    "esm": "^3.1.1",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "jest": "^25.0.0",
    "jest-sonar-reporter": "^2.0.0",
    "nodemon": "^1.18.10",
    "prettier": "^1.15.3",
    "proxyquire": "^2.1.0",
