    "common-tags": "^1.8.0",
    "connected-react-router": "^6.5.2",
    "date-fns": "^1.29.0",
    "debug": "^4.1.0",
    "ethereumjs-util": "^6.0.0",
    "fs-extra": "^7.0.1",
    "history": "^4.10.1",
    "inquirer": "^6.2.2",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.15",
    "moment-timezone": "^0.5.23",
    "node-sass": "^4.12.0",
    "number-to-words": "^1.2.4",
    "penpal": "^4.0.0",
    "prop-types": "^15.6.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-dropzone": "^7.0.1",
    "react-google-recaptcha": "^1.0.5",
    "react-helmet": "^5.2.1",
    "react-hot-loader": "^4.12.14",
    "react-qr-reader": "^2.2.1",
    "react-qr-svg": "^2.2.1",
    "react-redux": "^7.1.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-tabs": "^2.3.0",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.7",
