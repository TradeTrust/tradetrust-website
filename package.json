{
  "name": "tradetrust-website",
  "version": "2.0.0",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:update": "jest -u",
    "dev": "NODE_ENV=development npx webpack-dev-server",
    "lint": "eslint . --ext .js --max-warnings 0",
    "lint:fix": "eslint . --ext .js --fix",
    "build": "NODE_ENV=production webpack --progress --mode production",
    "dev-cli": "node -r esm ./scripts/cli.js",
    "serve-static": "http-server dist -s -p 3000",
    "integration": "testcafe chrome src/**/*.spec.js",
    "integration:headless": "testcafe chrome:headless src/**/*.spec.js",
    "integration:watch": "nodemon --exec \"npm run integration\" --watch src",
    "storybook": "start-storybook -s . -p 6006",
    "storybook:build": "build-storybook -s . -o docs"
  },
  "dependencies": {
