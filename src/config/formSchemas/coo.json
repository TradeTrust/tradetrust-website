{
  "name": "Certificate of Origin",
  "type": "VERIFIABLE_DOCUMENT",
  "defaults": {
    "@context": [
      "https://trustvc.io/context/coo.json",
      "https://trustvc.io/context/attachments-context.json"
    ],
    "renderMethod": 
      {
        "type": "EMBEDDED_RENDERER",
        "templateName": "CHAFTA_COO",
        "id": "https://generic-templates.tradetrust.io"
      }
    ,
    "credentialSubject": {
      "type": [
        "Coo"
      ]
    }
  },
  "schema": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "cooId": {
        "type": "string",
        "title": "COO ID"
      },
      "issueDateTime": {
        "type": "string",
        "title": "Issued Date & Time"
      },
      "firstSignatoryAuthentication": {
        "title": "Signatory Authentication",
        "type": "object",
        "properties": {
          "signature": {
            "type": "string",
            "title": "First Signatory"
          }
        }
      },
      "supplyChainConsignment": {
        "title": "Supply Chain Consignment",
        "type": "object",
        "properties": {
          "supplyChainConsignmentId": {
            "type": "string",
            "title": "ID"
          },
          "supplyChainConsignmentInformation": {
            "type": "string",
            "title": "Consignment Information"
          }
        }
      },
      "exportCountry": {
        "title": "Export Country",
        "type": "object",
        "properties": {
          "exportCountryCode": {
            "type": "string",
            "title": "Country Code"
          }
        }
      },
      "exporter": {
        "title": "Exporter",
        "type": "object",
        "properties": {
          "exporterId": {
            "title": "ID",
            "type": "string"
          },
          "exporterName": {
            "title": "Name",
            "type": "string"
          },
          "exporterLine1": {
            "type": "string",
            "title": "Address Line 1"
          },
          "exporterLine2": {
            "type": "string",
            "title": "Address Line 2"
          },
          "exporterCityName": {
            "type": "string",
            "title": "City"
          },
          "exporterPostcode": {
            "type": "string",
            "title": "Postal Code"
          },
          "exporterCountrySubDivisionName": {
            "type": "string",
            "title": "Country Sub Division Name"
          },
          "exporterCountryCode": {
            "type": "string",
            "title": "Country Code"
          }
        }
      },
      "importCountry": {
        "type": "object",
        "title": "Import Country",
        "properties": {
          "importCountryCode": {
            "type": "string",
            "title": "Country Code"
          }
        }
      },
      "importer": {
        "title": "Importer's Details (if known)",
        "type": "object",
        "properties": {
          "importerId": {
            "title": "Importer ID",
            "type": "string"
          },
          "importerName": {
            "title": "Name",
            "type": "string"
          },
          "importerLine1": {
            "type": "string",
            "title": "Address Line 1"
          },
          "importerLine2": {
            "type": "string",
            "title": "Address Line 2"
          },
          "importerCityName": {
            "type": "string",
            "title": "City"
          },
          "importerPostcode": {
            "type": "string",
            "title": "Postal Code"
          },
          "importerCountrySubDivisionName": {
            "type": "string",
            "title": "Country Sub Division Name"
          },
          "importerCountryCode": {
            "type": "string",
            "title": "Country Code"
          }
        }
      },
      "includedConsignmentItems": {
        "type": "array",
        "title": "Included Consignment Items",
        "items": {
          "type": "object",
          "properties": {
            "includedConsignmentItemsId": {
              "type": "string",
              "title": "ID"
            },
            "includedConsignmentItemsInformation": {
              "type": "string",
              "title": "Information"
            },
            "originCriteriaText": {
              "type": "string",
              "title": "Origin Criteria Text"
            },
            "manufacturer": {
              "type": "object",
              "title": "Manufacturer",
              "properties": {
                "manufacturerId": {
                  "type": "string",
                  "title": "ID"
                },
                "manufacturerName": {
                  "type": "string",
                  "title": "Name"
                },
                "manufacturerLine1": {
                  "type": "string",
                  "title": "Address Line 1"
                },
                "manufacturerLine2": {
                  "type": "string",
                  "title": "Address Line 2"
                },
                "manufacturerCityName": {
                  "type": "string",
                  "title": "City"
                },
                "manufacturerPostcode": {
                  "type": "string",
                  "title": "Postal Code"
                },
                "manufacturerCountrySubDivisionName": {
                  "type": "string",
                  "title": "Country Sub Division Name"
                },
                "manufacturerCountryCode": {
                  "type": "string",
                  "title": "Country Code"
                }
              }
            },
            "tradeLineItems": {
              "type": "array",
              "title": "Trade line Items",
              "items": {
                "type": "object",
                "properties": {
                  "sequenceNumber": {
                    "type": "integer",
                    "title": "Sequence Number"
                  },
                  "invoiceReference": {
                    "type": "object",
                    "title": "Invoice Reference",
                    "properties": {
                      "invoiceReferenceId": {
                        "type": "string",
                        "title": "ID"
                      },
                      "formattedIssueDateTime": {
                        "type": "string",
                        "title": "Formatted Issue Date & Time"
                      },
                      "attachedBinaryFileUri": {
                        "type": "string",
                        "title": "Attached Binary File URI"
                      }
                    }
                  },
                  "tradeProduct": {
                    "type": "object",
                    "title": "Trade Product",
                    "properties": {
                      "tradeProductId": {
                        "type": "string",
                        "title": "ID"
                      },
                      "tradeProductDescription": {
                        "type": "string",
                        "title": "Description"
                      },
                      "harmonisedTariffclassCode": {
                        "type": "string",
                        "title": "Harmonised Tariff Class Code"
                      },
                      "harmonisedTariffclassName": {
                        "type": "string",
                        "title": "Harmonised Tariff Class Name"
                      },
                      "originCountry": {
                        "type": "object",
                        "title": "Origin Country",
                        "properties": {
                          "originCountryCode": {
                            "type": "string",
                            "title": "Code"
                          }
                        }
                      }
                    }
                  },
                  "transportPackages": {
                    "type": "array",
                    "title": "Transport Packages",
                    "items": {
                      "type": "object",
                      "properties": {
                        "transportPackagesId": {
                          "type": "string",
                          "title": "ID"
                        },
                        "transportPackagesGrossVolume": {
                          "type": "string",
                          "title": "Gross Volume"
                        },
                        "transportPackagesGrossWeight": {
                          "type": "string",
                          "title": "Gross Weight"
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      },
      "loadingBaseportLocation": {
        "type": "object",
        "title": "Loading Baseport Location",
        "properties": {
          "loadingBaseportLocationId": {
            "type": "string",
            "title": "ID"
          },
          "loadingBaseportLocationName": {
            "type": "string",
            "title": "Name"
          }
        }
      },
      "mainCarriageTransportMovement": {
        "title": "Main Carriage Transport Movement",
        "type": "object",
        "properties": {
          "mainCarriageTransportMovementId": {
            "type": "string",
            "title": "ID"
          },
          "mainCarriageTransportMovementInformation": {
            "type": "string",
            "title": "Information"
          },
          "usedTransportMeansName": {
            "type": "string",
            "title": "Used Transport"
          },
          "usedTransportMeansId": {
            "type": "string",
            "title": "Used Transport ID"
          },
          "departureDateTime": {
            "type": "string",
            "title": "Departure Date and Time"
          }
        }
      },
      "unloadingBaseportLocation": {
        "type": "object",
        "title": "Unloading Baseport Location",
        "properties": {
          "unloadingBaseportLocationId": {
            "type": "string",
            "title": "ID"
          },
          "unloadingBaseportLocationName": {
            "type": "string",
            "title": "Name"
          }
        }
      }
    }
  },
  "uiSchema": {
    "issueDateTime": {
      "ui:widget": "datetime"
    },
    "firstSignatoryAuthentication": {
      "signature": {
        "ui:widget": "file",
        "ui:options": {
          "text": "Upload Signature",
          "accept": ".png, .jpeg, .jpg"
        }
      }
    },
    "includedConsignmentItems": {
      "items": {
        "tradeLineItems": {
          "items": {
            "invoiceReference": {
              "formattedIssueDateTime": {
                "ui:widget": "datetime"
              }
            }
          }
        }
      }
    },
    "mainCarriageTransportMovement": {
      "departureDateTime": {
        "ui:widget": "datetime"
      }
    }
  },
  "attachments": {
    "allow": true,
    "accept": {
      "application/pdf": [
        ".pdf"
      ]
    }
  },
  "extension": "tt"
}
